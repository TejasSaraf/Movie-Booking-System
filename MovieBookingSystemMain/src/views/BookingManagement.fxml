<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="567.0" prefWidth="1077.0" spacing="10"
	style="-fx-padding: 20;" xmlns="http://javafx.com/javafx/23.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.BookingManagement">
	<Button fx:id="homeBtn" mnemonicParsing="false"
		onAction="#handleHomeBtn" text="Home" translateX="964.0"
		translateY="18.0" />

	<Label style="-fx-font-size: 18px; -fx-font-weight: bold;"
		text="Booking Management" translateY="-16.0" />

	<TableView fx:id="bookingsTableView" prefHeight="410.0"
		prefWidth="994.0" VBox.vgrow="ALWAYS">
		<columns>
			<TableColumn fx:id="idColumn" text="Booking ID" />
			<TableColumn fx:id="firstnameColumn" text="First Name" />
			<TableColumn fx:id="lastnameColumn" text="Last Name" />
			<TableColumn fx:id="emailColumn" text="Email" />
			<TableColumn fx:id="filmTitleColumn" text="Film Title" />
			<TableColumn fx:id="screenNumColumn" prefWidth="56.0"
				text="Screen" />
			<TableColumn fx:id="dateColumn" prefWidth="75.0"
				text="Date" />
			<TableColumn fx:id="timeColumn" prefWidth="70.0"
				text="Time" />
			<TableColumn fx:id="adultTicketsColumn" prefWidth="85.0"
				text="Adult Tickets" />
			<TableColumn fx:id="childTicketsColumn" prefWidth="82.0"
				text="Child Tickets" />
			<TableColumn fx:id="seniorTicketsColumn" prefWidth="88.0"
				text="Senior Tickets" />
			<TableColumn fx:id="totalPriceColumn" prefWidth="74.0"
				text="Total Price" />
			<TableColumn fx:id="isVipColumn" prefWidth="52.0"
				text="VIP" />
		</columns>
	</TableView>
</VBox>
