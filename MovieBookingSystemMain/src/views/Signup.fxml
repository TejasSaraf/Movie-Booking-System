<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.SignUpController">
	<children>
		<HBox prefHeight="300.0" prefWidth="600.0">
			<children>
				<AnchorPane prefHeight="300.0" prefWidth="287.0">
					<children>
						<ImageView fitHeight="300.0" fitWidth="302.0"
							pickOnBounds="true" preserveRatio="true">
							<image>
								<Image url="@../images/Movie.png" />
							</image>
						</ImageView>
					</children>
				</AnchorPane>
				<AnchorPane prefHeight="300.0" prefWidth="299.0">
					<children>
						<Label layoutX="34.0" layoutY="40.0" text="FirstName" />
						<Label layoutX="36.0" layoutY="169.0" text="Password" />
						<Label layoutX="34.0" layoutY="129.0" text="Username" />
						<Label layoutX="35.0" layoutY="86.0" text="LastName" />
						<TextField fx:id="txtFirstName" layoutX="109.0"
							layoutY="36.0" promptText="Enter FirstName" />
						<TextField fx:id="txtPassword" layoutX="109.0"
							layoutY="165.0" promptText="Enter Password" />
						<TextField fx:id="txtUserName" layoutX="109.0"
							layoutY="125.0" promptText="Enter Username" />
						<TextField fx:id="txtLastName" layoutX="109.0"
							layoutY="82.0" promptText="Enter LastName" />
						<Button fx:id="signUpButton" layoutX="127.0"
							layoutY="215.0" mnemonicParsing="false"
							onAction="#signUpButtonOnAction"
							style="-fx-background-color: #b93939;" text="SignUp"
							textAlignment="CENTER" textFill="#f8f8f8">
							<font>
								<Font name="System Bold" size="12.0" />
							</font>
						</Button>
						<Label layoutX="76.0" layoutY="253.0"
							text="Already a member?" />
						<Button fx:id="loginButton" layoutX="179.0" layoutY="249.0"
							mnemonicParsing="false" onAction="#loginButtonOnAction"
							style="-fx-background-color: transperent;" text="Login"
							textAlignment="CENTER" textFill="#4f3cd0" underline="true" />
						<Label fx:id="lblStatus" layoutX="145.0" layoutY="190.0"
							prefHeight="17.0" prefWidth="20.0" />
						<Label layoutX="135.0" layoutY="6.0" text="SignUp">
							<font>
								<Font name="System Bold" size="14.0" />
							</font>
						</Label>
					</children>
				</AnchorPane>
			</children>
		</HBox>
	</children>
</AnchorPane>
